{
  "name": "@aisoft/backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prepare": "husky install",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build-bundle": "npm run prebuild && nest build && javascript-obfuscator ./dist --output ./dist-bundle && cp -r dist-bundle/* dist && rm -rf dist-bundle",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix --cache --cache-strategy content --cache-location ./node_modules/.cache/eslint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@golevelup/nestjs-modules": "^0.6.1",
    "@golevelup/nestjs-rabbitmq": "^3.6.0",
    "@grpc/grpc-js": "^1.8.14",
    "@grpc/proto-loader": "^0.7.7",
    "@nestjs/bull": "^0.6.2",
    "@nestjs/common": "^9.2.1",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.2.1",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/microservices": "^9.2.1",
    "@nestjs/mongoose": "^9.2.2",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.4.0",
    "@nestjs/swagger": "^6.1.4",
    "bcryptjs": "^2.4.3",
    "bull": "^4.10.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "jose": "^4.14.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "mongoose": "^7.3.1",
    "mongoose-lean-defaults": "^2.2.1",
    "mongoose-lean-getters": "^1.1.0",
    "mongoose-lean-virtuals": "^0.9.1",
    "nestjs-asyncapi": "^1.1.1",
    "nestjs-i18n": "^10.2.6",
    "npm": "^9.6.7",
    "onesignal-node": "^3.4.0",
    "p-limit": "^3.1.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "protobufjs": "^7.2.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "ua-parser-js": "^1.0.33",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.2.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/bull": "^4.10.0",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.8",
    "@types/lodash": "^4.14.194",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.11",
    "@types/ua-parser-js": "^0.7.36",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.3",
    "javascript-obfuscator": "^4.0.2",
    "jest": "28.1.3",
    "lint-staged": "^13.1.2",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "moduleNameMapper": {
      "^@common(.*)$": "<rootDir>/common$1",
      "^@config(.*)$": "<rootDir>/config$1",
      "^@module(.*)$": "<rootDir>/modules$1"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.ts": [
      "npm run format",
      "npm run lint"
    ]
  }
}
